{
  "version": 2,
  "projects": {
    "material": {
      "projectType": "library",
      "root": "packages/material",
      "sourceRoot": "packages/material/src",
      "prefix": "ngx-declarative",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "packages/material/tsconfig.lib.json",
            "project": "packages/material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/material/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/material/tsconfig.lib.json",
              "packages/material/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/material/**/*"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/material"
          ],
          "options": {
            "jestConfig": "packages/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "material-demo": {
      "projectType": "application",
      "root": "apps/material-demo",
      "sourceRoot": "apps/material-demo/src",
      "prefix": "ngx-declarative",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/material-demo",
            "index": "apps/material-demo/src/index.html",
            "main": "apps/material-demo/src/main.ts",
            "polyfills": "apps/material-demo/src/polyfills.ts",
            "tsConfig": "apps/material-demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/material-demo/src/favicon.ico",
              "apps/material-demo/src/assets"
            ],
            "styles": [
              "apps/material-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/material-demo/src/environments/environment.ts",
                  "with": "apps/material-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "material-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "material-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "material-demo:build"
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/material-demo/tsconfig.app.json",
              "apps/material-demo/tsconfig.spec.json",
              "apps/material-demo/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/material-demo/**/*"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/material-demo"
          ],
          "options": {
            "jestConfig": "apps/material-demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "material"
}